// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../database.db"
}

model Coloborador_Login {
    id Int @id @default(autoincrement())
    name String
    senha String
}

model Coloborador {
    id Int @id @default(autoincrement())
    name String
    admission String
    role String
    type_blood String
}

